check host nodejs with address 127.0.0.1
  start program = "<%= @start_program %>"
  stop program = "<%= @stop_program %>"
  <% if @uid -%>as uid <%= @uid %><% end -%>
  <% if @gid -%>as gid <%= @gid %><% end -%>
  group Pushit
  if failed port <%= @upstream_port %> protocol HTTP
    request /
    with timeout 10 seconds
    then restart
